# baseURI: http://topbraid.org/examples/shaclsquare
# imports: http://www.w3.org/ns/shacl
# prefix: ex

@prefix : <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix inst: <http://example.org/instance#> .
@prefix wfonto: <http://data.webfoundation.org/ontology/> . 
@prefix dcterms: <http://purl.org/dc/terms/> . 
@prefix qb: <http://purl.org/linked-data/cube#> .

:ObservationShape
	a sh:Shape ;
	sh:scopeClass wfonto:Observation ;
	sh:property [
		sh:predicate rdf:type ;
		sh:minCount 2 ;
		sh:maxCount 2 ;
		sh:qualifiedValueShape [
			a sh:Shape ;   # Optional
			sh:property [
				sh:predicate rdf:type ;
				sh:hasValue qb:Observation ;
			]
		] ;
		 # sh:qualifiedMinCount 1 ;
	] ;
	sh:property [
		sh:predicate rdfs:label ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:predicate dcterms:issued ;
		sh:datatype xsd:integer ;
		sh:minCount 0 ;
		sh:maxCount 1 ;
	] .


<> rdf:type sh:Graph ;
   rdfs:comment "WebIndex data" ;
   rdfs:label "SHACL Example for WebIndex" ;
   sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.

